{
    "name": "@juicyllama/llana",
    "version": "0.0.1",
    "description": "Low-code API Generator: Use your database schema to automatically create and maintain a NestJs CRUD+ API application",
    "author": {
        "name": "JuicyLlama Studio",
        "email": "studio@juicyllama.com",
        "url": "https://juicyllama.com"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/juicyllama/llana"
    },
    "bugs": "https://llana.io",
    "license": "BSD-4-Clause",
    "readmeFilename": "README.md",
    "bin": {
        "llana": "./cli.ts"
    },
    "postInstall": "npm link && llana boot && llana install",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write .",
        "lint": "eslint --fix",
        "start": "ts-node --esm cli.ts"
    },
    "dependencies": {
        "child-process-promise": "^2.2.1",
        "dotenv": "^16.4.5",
        "ipt": "^3.2.0",
        "mysql2": "^3.11.0",
        "mysql2-promise": "^0.1.4",
        "prompt-sync": "^4.2.0",
        "read-pkg": "^9.0.1",
        "simple-output": "^3.0.0",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "@types/child-process-promise": "^2.2.6",
        "@types/node": "^22.5.1"
    }
}
